    "def isPrime(n): \n",
    "    if (n <= 1): \n",
    "        return False \n",
    "    for i in range(2, n): \n",
    "        if (n % i == 0): \n",
    "            return False\n",
    "    return True\n",
    "if isPrime(11): \n",
    "    print (\"true\") \n",
    "else: \n",
    "    print (\"false\")\n",
    "  "
